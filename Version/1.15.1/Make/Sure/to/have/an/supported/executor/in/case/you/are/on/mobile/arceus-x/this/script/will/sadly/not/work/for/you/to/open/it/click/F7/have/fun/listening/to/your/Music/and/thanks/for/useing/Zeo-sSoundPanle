local function NVPICZK_fake_script() -- ZeosSoundPanle.kb 
	local script = Instance.new('LocalScript', ZeosSoundPanle)

	local MainFrame = script.Parent.Main
	local Opened = false
	
	local PositionClosed = UDim2.new(0.375, 0,-1.394, 0)
	local PositionOpened = UDim2.new(0.375, 0,0.408, 0)
	
	local UIS = game:GetService("UserInputService")
	
	UIS.InputBegan:Connect(function(KeyCode)
		if KeyCode.KeyCode == Enum.KeyCode.F8 then
			if Opened then
				MainFrame:TweenPosition((PositionClosed), "InOut", "Quint")
				Opened = false
			else
				MainFrame:TweenPosition((PositionOpened), "InOut", "Quart")
				Opened = true
			end
		end
	end)
end
coroutine.wrap(NVPICZK_fake_script)()
