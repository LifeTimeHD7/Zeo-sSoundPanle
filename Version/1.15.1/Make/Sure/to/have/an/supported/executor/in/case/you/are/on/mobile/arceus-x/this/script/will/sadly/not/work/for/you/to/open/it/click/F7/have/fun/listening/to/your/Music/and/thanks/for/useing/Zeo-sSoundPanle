local function HRAUH_fake_script() -- ZeosSoundPanle.HowTo 
	local script = Instance.new('LocalScript', ZeosSoundPanle)

	print("|How to Use Zeo's SoundPanle|")
	wait(0.01)
	print("")
	wait()
	print("| input any Sound id into SoundId to Start Playing The Music | input a Value into Pitch to Give it a Specific Speed |")
	wait()
	print("| input a Value into Volume to Give it a Specific Volume | input a Value into TimePosition to Skip Parts |")
	wait()
	print("| By: Zeo | Powered by: Core Sounds | Version: 1.15.1 |")
	wait()
	print("        ")
	wait(1)
	print(
		"| Created Date: 2/17/2022 |",
		"| Functions: Enabled |",
		"| PanleSound: Enabled |",
		
		"| Commands: Disabled |",
		"| Cords: Disabled |",
		"| Social Links: Disabled |",
		"| Html Request: Disabled |",
		
		"| LocalScripts: Enabled |",
		
		"| Hight Suspicious 'Anti-Cheat','Clevear Bot' Detacted |",
		
		warn("| 'Anti-Cheat','Cleaver Bot': Destroy() |"),
		wait(0),
		"| Success! |",
		
		"| End |"
	)
	
end
