-- Scripts:

local function OKPKKQ_fake_script() -- SoundId.Effects&Functions 
	local script = Instance.new('LocalScript', SoundId)

	--[[ Functions ]]--
	
	----------------------------------------------------------
	
	script.Parent.MouseButton1Click:Connect(function()
			wait(1)
			game.SoundService.PanleSound:Play()
		end)
	
	script.Parent.MouseButton1Click:Connect(function()
			game.SoundService.PanleSound.SoundId ="rbxassetid://"..script.Parent.input.Text..""
		end)
	
	----------------------------------------------------------
	
	--[[ Effects ]]--
	
	----------------------------------------------------------
	
	script.Parent.MouseEnter:Connect(function()
		script.Parent.BorderSizePixel = 2
	end)
	
	script.Parent.MouseLeave:Connect(function()
		script.Parent.BorderSizePixel = 1
	end)
	
	----------------------------------------------------------
	
	script.Parent.MouseEnter:Connect(function()
		script.Parent.BorderColor3 = Color3.fromRGB(255, 255, 255)
	end)
	
	script.Parent.MouseLeave:Connect(function()
		script.Parent.BorderColor3 = Color3.fromRGB(255, 255, 255)
	end)
	
	----------------------------------------------------------
	
	script.Parent.MouseEnter:Connect(function()
		script.Parent.TextColor3 = Color3.fromRGB(255, 255, 255)
	end)
	
	script.Parent.MouseLeave:Connect(function()
		script.Parent.TextColor3 = Color3.fromRGB(255, 255, 255)
	end)
	
	----------------------------------------------------------
	
	script.Parent.MouseEnter:Connect(function()
		script.Parent.input.BorderSizePixel = 2
	end)
	
	script.Parent.MouseLeave:Connect(function()
		script.Parent.input.BorderSizePixel = 1
	end)
end
coroutine.wrap(OKPKKQ_fake_script)()
local function VGBLSAP_fake_script() -- input_2.Text 
	local script = Instance.new('LocalScript', input_2)

	script.Parent.Text = game.SoundService.PanleSound.PlaybackSpeed
end
coroutine.wrap(VGBLSAP_fake_script)()
