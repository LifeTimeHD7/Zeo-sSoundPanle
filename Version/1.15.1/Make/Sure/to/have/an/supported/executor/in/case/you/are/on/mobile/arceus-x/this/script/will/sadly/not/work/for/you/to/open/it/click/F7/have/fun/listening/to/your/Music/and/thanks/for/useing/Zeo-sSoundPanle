local function QJLMB_fake_script() -- Effects_2.Effects&Functions 
	local script = Instance.new('LocalScript', Effects_2)

	--[[ Functions ]]--
	
	----------------------------------------------------------
	
	script.Parent.MouseButton1Click:Connect(function()
		local RSE = Instance.new("ReverbSoundEffect")
		
		wait(1)
		RSE.Parent = game.SoundService.PanleSound
		RSE.Name ="RSE"
	end)
	
	----------------------------------------------------------
	
	--[[ Effects ]]--
	
	----------------------------------------------------------
	
	script.Parent.MouseEnter:Connect(function()
		script.Parent.BorderSizePixel = 2
